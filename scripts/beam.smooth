d4 $ (striate (every 2 ((1%2) <~) (sound "[bd h/3] [bd kurt/2 kurt bd]")) 3)
  ~~ pan "1 0"
  ~~ vowel "e"

d1 $ sound (cat ["[~ [sn jvbass] ~ [sn, jvbass]]",
                density (1%3) "[~ [sn jvbass] ~ [sn, jvbass]]"
               ])
  ~~ pan "0 1"

d3 $ sound "[bd wobble bd] wobble"
  ~~ speed "0.8"

d5 $ sound "bd sn/4 [cp sn/4]"

